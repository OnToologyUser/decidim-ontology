         rdfs:range :ParticipationItem ;
         rdfs:label "reports"@en .


###  http://ontology.decidim.org/decidim_roadmap/result_of
:result_of rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :refers_to ;
           rdfs:domain :Result ;
           rdfs:range :Proposal ;
           rdfs:label "result of"^^xsd:string .


###  http://ontology.decidim.org/decidim_roadmap/role_of
:role_of rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :specifies ;
         rdfs:domain :Role ;
         rdfs:range <http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant> ;
         rdfs:label "role of"^^xsd:string .


###  http://ontology.decidim.org/decidim_roadmap/role_scope_of
:role_scope_of rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :specifies ;
               rdfs:domain :ParticipationArtifact ;
               rdfs:range :Role ;
               rdfs:label "role scope of"^^xsd:string .


###  http://ontology.decidim.org/decidim_roadmap/scope_of
:scope_of rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :specifies ;
          rdfs:label "scope of"^^xsd:string .


###  http://ontology.decidim.org/decidim_roadmap/similar_to
:similar_to rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :related_to ;
            rdfs:domain :ParticipationItem ;
            rdfs:range :ParticipationItem ;
            rdfs:comment "Symmetric object property that indicates that two Participation items (e.g., two Proposals) are similar to each other."@en .


###  http://ontology.decidim.org/decidim_roadmap/specified_by
:specified_by rdf:type owl:ObjectProperty ;
              owl:inverseOf :specifies ;
              rdfs:comment "General property that indicates that some aspect or attribute of the entity is specified by an other one."@en ;
              rdfs:label "specified by"@en .


###  http://ontology.decidim.org/decidim_roadmap/specifies
:specifies rdf:type owl:ObjectProperty ;
           rdfs:comment """General property that indicates that an entity specifies an aspect or attribute of another entity, such as its Scope, Category or Role.

It is inverse property of specified_by."""@en ;
           rdfs:label "specifies"@en .


###  http://ontology.decidim.org/decidim_roadmap/subcategory_of
:subcategory_of rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :belong_to ;
                rdfs:domain :Category ;
                rdfs:range :Category ;
                rdfs:label "subcategory of"@en .


###  http://ontology.decidim.org/decidim_roadmap/subgroup_of
:subgroup_of rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :belong_to ;
             rdfs:domain :Group ;
             rdfs:range :Group ;
             rdfs:label "subgroup of"@en .


###  http://ontology.decidim.org/decidim_roadmap/supporter_of
:supporter_of rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :acts_on ;
              rdfs:domain :VerifiedPerson ;
              rdfs:range :Proposal ;
              rdfs:label "supporter of"@en ,
                         "supports"^^xsd:string .


###  http://ontology.decidim.org/decidim_roadmap/votes_comment
:votes_comment rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :acts_on ;
               rdfs:domain <http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant> ;
               rdfs:range :Comment ;
               rdfs:label "votes comment"^^xsd:string .


###  http://ontology.decidim.org/decidim_roadmap/withdrawn_by
:withdrawn_by rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :has_actor ;
              owl:inverseOf :withdraws ;
              rdfs:domain <http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant> ;
              rdfs:range :Proposal ;
              rdfs:label "withdrawn by"@en .


###  http://ontology.decidim.org/decidim_roadmap/withdraws
:withdraws rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :acts_on ;
           rdfs:domain <http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant> ;
           rdfs:range :Proposal ;
           rdfs:label "withdraws"@en .


#################################################################
#    Classes
#################################################################

###  http://ontology.decidim.org/decidim_roadmap/AccountabilityContainer
:AccountabilityContainer rdf:type owl:Class ;
                         rdfs:subClassOf :Container ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty :container_of ;
                                           owl:someValuesFrom :Result
                                         ] ;
                         rdfs:comment """This functionality allows participants to monitor the results of a participation space, and the progress of derived projects.

It is a ParticipationFunctionality of kind Container, and includes Items of kind Result, vinculated to the corresponding accepted Proposals and to projects deriving from them."""@en ;
                         rdfs:label "Accountability container"@en .


###  http://ontology.decidim.org/decidim_roadmap/Administrator
:Administrator rdf:type owl:Class ;
               rdfs:subClassOf :Moderator ;
               rdfs:comment "A registered participant who has the role of Administrator on a ParticipationArtifact can configure and manage it, and can grant other RegisteredParticipants administration permissions of Collaborator, Moderator or Administrator in the same ParticipationArtifact or on a ParticipationArtifact contained in it."@en ;
               rdfs:label "Administrator"@en .


###  http://ontology.decidim.org/decidim_roadmap/Amendment
:Amendment rdf:type owl:Class ;
           rdfs:subClassOf :Proposal ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :amendment_of ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :Proposal
                           ] ;
           rdfs:comment "An Amendment is a kind of Proposal that refers to a specific Proposal it proposes to enmend, to which it is linked through the property amendment_of."@en ;
           rdfs:label "Amendment"@en .


###  http://ontology.decidim.org/decidim_roadmap/Assembly
:Assembly rdf:type owl:Class ;
          rdfs:subClassOf :Group ,
                          <http://ontology.decidim.org/decidim_roadmap/#ParticipationSpace> ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :has_promoter ;
                            owl:someValuesFrom :SovereignOrganization
                          ] ;
          owl:disjointWith :Consultation ,
                           :Initiative ,
                           :Process ,
                           :ProcessGroup ;
          rdfs:comment """An assembly is a participation space, defined as a structured group of participants that meet periodically, individually or representatively, and can create proposals and make decisions. Beyond being a Participation space, an assembly is also a Group, and as such it can be the promoter of a Process, or participate in other participation spaces.

An Assembly may have WorkingGroups, i.e. subgroups created to work on specific issues."""@en ;
          rdfs:label "Assembly"@en .


###  http://ontology.decidim.org/decidim_roadmap/Attribute
:Attribute rdf:type owl:Class ;
           rdfs:comment "General class for entities that represent an attribute of other entities, such as their Scope or Category."@en ;
           rdfs:label "Attribute"@en .


###  http://ontology.decidim.org/decidim_roadmap/AuthoredItem
:AuthoredItem rdf:type owl:Class ;
              rdfs:subClassOf :ParticipationItem ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :has_creator ;
                                owl:someValuesFrom <http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant>
                              ] ;
              rdfs:comment "This class represents Items that have an author. While some content in the Platform and ParticipationSpaces is created by the Adinistrators, AuthoredItems have a creator that can typically be any RegisteredParticipant."@en ;
              rdfs:label "Authored resource"@en .


###  http://ontology.decidim.org/decidim_roadmap/Blog
:Blog rdf:type owl:Class ;
      rdfs:subClassOf :Container ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :container_of ;
                        owl:someValuesFrom :Blog_post
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :container_of ;
                        owl:someValuesFrom :Thread
                      ] ;
      rdfs:comment """A Blog allows for the reation of pieces of content (BlogPosts) in shown in chronological order and classified according to the patform's content categorization system.

A Blog is a ParticipationFunctionality of kind Container, that includes Items of kind BlogPost. Blog posts can in turn receive comments and conversation Threads, analogously to Proposals and other Items."""@en ;
      rdfs:label "Blog"@en .


###  http://ontology.decidim.org/decidim_roadmap/Blog_post
:Blog_post rdf:type owl:Class ;
           rdfs:subClassOf :ParticipationItem ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :has_container ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :Blog
                           ] ;
           rdfs:comment "A BlogPost is a piece of content that is published within a Blog."@en ;
           rdfs:label "Blog post"@en .


###  http://ontology.decidim.org/decidim_roadmap/BudgetedProject
:BudgetedProject rdf:type owl:Class ;
                 rdfs:subClassOf :AuthoredItem ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :has_container ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass :ParticipativeBudget
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :has_creator ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass <http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant>
                                 ] ;
                 rdfs:comment "A kind of proposal, that has a budget associated with it."@en ;
                 rdfs:label "Budgeted project"@en .


###  http://ontology.decidim.org/decidim_roadmap/Category
:Category rdf:type owl:Class ;
          rdfs:subClassOf :Attribute ;
          rdfs:comment "Categories are used to group items according to some criteria, typically the topic. They help participants browse and search information in a Participation space."@en ;
          rdfs:label "Category"@en .


###  http://ontology.decidim.org/decidim_roadmap/Collaborator
:Collaborator rdf:type owl:Class ;
              rdfs:subClassOf :Role ;
              rdfs:comment "A RegisteredParticipant having the Role of Collaborator in a given role scope can access the same information as an Administrator, including configuration panels, but cannot modify it."@en ;
              rdfs:label "Collaborator"@en .


###  http://ontology.decidim.org/decidim_roadmap/Comment
:Comment rdf:type owl:Class ;
         rdfs:subClassOf :AuthoredItem ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :has_creator ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass <http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant>
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :reply_of ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :ParticipationItem
                         ] ;
         rdfs:comment """Comments are ParticipationItems authored by RegisteredParticipants, that can be associated with different ParticipationFunctionalities and Items. 

A Comment can be referred to another comment (NestedComment) or to some other ParticipationItem (FirstLevelComment)."""@en ;
         rdfs:label "Comment"@en .


###  http://ontology.decidim.org/decidim_roadmap/Configurator
:Configurator rdf:type owl:Class ;
              rdfs:subClassOf :ParticipationArtifact ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :configurator_of ;
                                owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass :ParticipationArtifact
                              ] ;
              rdfs:comment """Administration panel associated to a ParticipationArtifact, that allows Administrators to configure it. Collaborators of the artifat can access the configurator but not modify it; moderators can also access the configurator, and additionally they can manage certain aspects such as accepting or rejecting proposals.
A Configurator is linked to the ParticipationArtifacts it allows to configure through the property configurator_of."""@en ;
              rdfs:label "Configurator"@en .


###  http://ontology.decidim.org/decidim_roadmap/Consultation
:Consultation rdf:type owl:Class ;
              rdfs:subClassOf <http://ontology.decidim.org/decidim_roadmap/#ParticipationSpace> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :has_promoter ;
                                owl:someValuesFrom :SovereignOrganization
                              ] ;
              owl:disjointWith :Initiative ,
                               :Process ,
                               :ProcessGroup ;
              rdfs:comment "A Consultation is a ParticipationSpace that allows a votation on some issue, involving all the participants of the SovereignOrganization promoting it. Participants can not only vote, but also get information, debate and monitor the results."@en ;
              rdfs:label "Consultation"@en .


###  http://ontology.decidim.org/decidim_roadmap/Container
:Container rdf:type owl:Class ;
           rdfs:subClassOf :ParticipationFunctionality ;
           rdfs:comment """Functionalities of this kind contain ParticipationItems, to which they are linked to the property container_of. 

Different kinds of ParticipationContainers contain different kinds of ParticipationItems: for example, a Debate contains items of ind Thread and Comment, a ProposalIncubator contains Proposals, a ParticipativeBudget contains BudgetedProposals and a MeetingCalendar contains Meetings."""@en ;
           rdfs:label "Participation container"@en .


###  http://ontology.decidim.org/decidim_roadmap/Debate
:Debate rdf:type owl:Class ;
        rdfs:subClassOf :Container ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :container_of ;
                          owl:someValuesFrom :Blog_post
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :container_of ;
                          owl:someValuesFrom :Thread
                        ] ;
        rdfs:comment """This functionality allows for the creation of debates about specific questions, defined by the Administrators or by RegisteredParticipants.

A Debate is a ParticipationFunctionality of kind Container, that includes Items of kind Comment, and the corresponding Threads."""@en ;
        rdfs:label "Debate"@en .


###  http://ontology.decidim.org/decidim_roadmap/FirstLevelComment
:FirstLevelComment rdf:type owl:Class ;
                   rdfs:subClassOf :Comment ;
                   owl:disjointWith :NestedComment ;
                   rdfs:comment "A comment referred to some ParticipationItem, which is not another comment."@en ;
                   rdfs:label "First level comment"@en .


###  http://ontology.decidim.org/decidim_roadmap/Group
:Group rdf:type owl:Class ;
       rdfs:subClassOf :Organization ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :has_member ;
                         owl:someValuesFrom <http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant>
                       ] ;
       owl:disjointWith :RegisteredPerson ;
       rdfs:comment "A collective account corresponding to a group of registered participants."@en ;
       rdfs:label "Group"@en .


###  http://ontology.decidim.org/decidim_roadmap/Initiative
:Initiative rdf:type owl:Class ;
            rdfs:subClassOf <http://ontology.decidim.org/decidim_roadmap/#ParticipationSpace> ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :has_promoter ;
                              owl:someValuesFrom <http://ontology.decidim.org/decidim_roadmap/#VerifiedParticipant>
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :initiative_of ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass :Proposal
                            ] ;
            owl:disjointWith :Process ,
                             :ProcessGroup ;
            rdfs:comment """While a Process is necessarily promoted by an organization, an Initiative is a ParticipationSpace that can be promoted by any VerifiedParticipant, i.e. by any individual citizen or organization whose account has been verified to correspond to their real identity. 

An Initiative is always connected to a Proposal it promotes, with the goal of gathering support for it, and to be considered for acceptance by the SovereignOrganization running the ParticipationPlatform."""@en ;
            rdfs:label "Initiative"@en .


###  http://ontology.decidim.org/decidim_roadmap/ManagedAccount
:ManagedAccount rdf:type owl:Class ;
                rdfs:subClassOf :VerifiedPerson ;
                rdfs:comment "A ManagedAccount is an account managed on behalf of another Person, that for some reason is not able to manage it by themselves."@en ;
                rdfs:label "Managed account"@en .


###  http://ontology.decidim.org/decidim_roadmap/Meeting
:Meeting rdf:type owl:Class ;
         rdfs:subClassOf :ParticipationItem ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :has_attendee ;
                           owl:someValuesFrom <http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant>
                         ] ;
         rdfs:comment """A Meeting is a physical gathering in which RegisteredParticipants can meet presentially to discuss about some issue and create Proposals. 

A Meeting is linked to the participants attending it through the property has_attendee.

A Meeting can be included in a Workshop or in an EventCalendar associated to some ParticipationSpace."""@en ;
         rdfs:label "Meeting"@en .


###  http://ontology.decidim.org/decidim_roadmap/MeetingCalendar
:MeetingCalendar rdf:type owl:Class ;
                 rdfs:subClassOf :Container ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty :container_of ;
                                   owl:someValuesFrom :Meeting
                                 ] ;
                 rdfs:comment """A MeetingCalendar is a group of meetings associated to a given ParticipationSpace. 

Like a Workshop, it is a ParticipationFunctionality of kind Container, that includes Item of kind Meeting; however, in this case events may typically span over a long period of time, be located in different places and not be related to each other."""@en ;
                 rdfs:label "Meeting calendar"@en .


###  http://ontology.decidim.org/decidim_roadmap/Moderator
:Moderator rdf:type owl:Class ;
           rdfs:subClassOf :Collaborator ;
           rdfs:comment "The role of Moderator attributes to a RegisteredParticipant the same rights as a Collaborator for accessing information, plus the possibility of managing ParticipationArtifacts within the role's scope (for example, they can assign a status to a Proposal, and provide an official response)."@en ;
           rdfs:label "Moderator"@en .


###  http://ontology.decidim.org/decidim_roadmap/NestedComment
:NestedComment rdf:type owl:Class ;
               rdfs:subClassOf :Comment ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :reply_of ;
                                 owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onClass :Comment
                               ] ;
               rdfs:comment "A comment that replies to another comment."@en ;
               rdfs:label "Nested comment"@en .


###  http://ontology.decidim.org/decidim_roadmap/Newsletter
:Newsletter rdf:type owl:Class ;
            rdfs:subClassOf :ParticipationFunctionality ;
            rdfs:comment "A Newsletter allows to broadcast messages to all the RegisteredParticipants associated to a ParticipationPlatform or to a specific ParticipationSpace."@en ;
            rdfs:label "Newsletter"@en .


###  http://ontology.decidim.org/decidim_roadmap/OfficialAccount
:OfficialAccount rdf:type owl:Class ;
                 rdfs:subClassOf :VerifiedPerson ;
                 rdfs:comment "A verified person whose account name has also been verified to correspond to the real name."@en ;
                 rdfs:label "Official account"^^xsd:string .


###  http://ontology.decidim.org/decidim_roadmap/Organization
:Organization rdf:type owl:Class ;
              rdfs:subClassOf <http://ontology.decidim.org/decidim_roadmap/#Agent> ;
              owl:disjointWith :Person ;
              rdfs:comment "An organized group of people with a collective purpose. Specific subclasses represent the case of groups of registered participants (Group), and the organization promoting a ParticipationPlatform or a ParticipationSpace (Sovereign organization)."@en ;
              rdfs:label "Organization"@en .


###  http://ontology.decidim.org/decidim_roadmap/Page
:Page rdf:type owl:Class ;
      rdfs:subClassOf :ParticipationFunctionality ;
      rdfs:comment "This ParticipationFunctionality allows to create informative pages with enriched text, images or videos."@en ;
      rdfs:label "Page"@en .


###  http://ontology.decidim.org/decidim_roadmap/ParticipationArtifact
:ParticipationArtifact rdf:type owl:Class ;
                       rdfs:comment "General class including the various elements that allow democratic participation in Decidim: ParticipationPlatforms, ParticipationSpaces (such as Participation processes or Assemblies), ParticipationFunctionalities (such as Debates, Surveys or Votations), and ParticipationItems (such as individual proposals, meetings or comments)."@en ;
                       rdfs:label "Artifact"@en .


###  http://ontology.decidim.org/decidim_roadmap/ParticipationFunctionality
:ParticipationFunctionality rdf:type owl:Class ;
                            rdfs:subClassOf :ParticipationArtifact ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :functionality_of ;
                                              owl:someValuesFrom <http://ontology.decidim.org/decidim_roadmap/#ParticipationSpace>
                                            ] ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :has_configurator ;
                                              owl:someValuesFrom :Configurator
                                            ] ;
                            rdfs:comment """This class includes the different functionalitites that can be added to a ParticipationSpace by RegisteredParticipants having administrative role to configure it. 

Each ParticipationFunctionality has to be linked to at least one participationSpace it belongs to, through the property functionality_of.
Furthermore, a ParticipationFunctionality needs to have a Configurator that allows its configuration by participants having the permissions to do so.

Some functionalitites represent one single element that is added to the ParticipationSpace, such as a Page, a Votation or a Survey, while other functionalities are elements that contain participationItems, such as Proposals, BudgetedProposals, Meetings or Comments; such functionalitties that contain ParticipationItems are defined as ParticipationContainers.

While some functionalities are general and can belong to any kind of ParticipationSpace, such as a Page, a Blog or a Debate, other are specific to a certain kind of participationSpace. E.g., a WorkingGroup can only belong to an Assembly. In the latter case, the range of the property functionality_of for that particular ParticipationFunctionality is restrictetd to the kind of space it can belong to."""@en ;
                            rdfs:label "Component"@en .


###  http://ontology.decidim.org/decidim_roadmap/ParticipationItem
:ParticipationItem rdf:type owl:Class ;
                   rdfs:subClassOf :ParticipationArtifact ;
                   rdfs:comment """This general class represents all individual items within a participation space such as Proposals, Comments, meetings, Blog posts.

Items are typically contained in some ParticipationContainer associated with a ParticipationSpace, and different kinds of Items are typically contained in different kinds of containers. For example, Bog posts in Blogs, Proposals in ProposalContainers, Meetings in Workshops or MeetingCalendars, and so on."""@en ;
                   rdfs:label "Resource"@en .


###  http://ontology.decidim.org/decidim_roadmap/ParticipationPlatform
:ParticipationPlatform rdf:type owl:Class ;
                       rdfs:subClassOf :ParticipationArtifact ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :has_promoter ;
                                         owl:someValuesFrom :SovereignOrganization
                                       ] ;
                       rdfs:comment """This class represent instances of a participation platform. 

It can include different kinds of ParticipationSpaces, such as Assemblies, Processes, Consultations or Initiatives; all ParticipationArtifacts ultimately relate to some platform."""@en ;
                       rdfs:label "Platform"@en .


###  http://ontology.decidim.org/decidim_roadmap/ParticipativeBudget
:ParticipativeBudget rdf:type owl:Class ;
                     rdfs:subClassOf :Container ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :container_of ;
                                       owl:someValuesFrom :BudgetedProject
                                     ] ;
                     rdfs:comment """This component allows participants to collectively define a budget by prioritizing budgeted projects.

It is a ParticipationFunctionality of kind Container, and includes Items of kind BudgetedProposal."""@en ;
                     rdfs:label "Participative budget"@en .


###  http://ontology.decidim.org/decidim_roadmap/ParticipativeText
:ParticipativeText rdf:type owl:Class ;
                   rdfs:subClassOf :ProposalContainer ;
                   rdfs:comment """This functionality allows participants to collaboratively redact a text as a collection of Proposals. Collaborative editing emerges from interaction of participants with the Proposals that compose the text. 

This functionality is a special kind of ProposalContainer, that composes proposals within a unified text, or decomposes a text into various proposals that can be discussed and prioritized separately.

A ParticipativeText is a Functionality of kind Container, that includes Items of kind Proposal."""@en ;
                   rdfs:label "Participative text"@en .


###  http://ontology.decidim.org/decidim_roadmap/Person
:Person rdf:type owl:Class ;
        rdfs:subClassOf <http://ontology.decidim.org/decidim_roadmap/#Agent> ;
        rdfs:comment "General class that represents any person as an individual agent, opposed to collective agents (Organizations)."@en ;
        rdfs:label "Person"@en .


###  http://ontology.decidim.org/decidim_roadmap/Phase
:Phase rdf:type owl:Class ;
       rdfs:subClassOf :ParticipationFunctionality ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :phase_of ;
                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass :Process
                       ] ;
       rdfs:comment "This functionality allows the administrators of a Process to design and different phases in which the process is divided, with a start and an end date, and with different actions and permissions for each phase."@en ;
       rdfs:label "Phase"@en .


###  http://ontology.decidim.org/decidim_roadmap/Process
:Process rdf:type owl:Class ;
         rdfs:subClassOf <http://ontology.decidim.org/decidim_roadmap/#ParticipationSpace> ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :has_promoter ;
                           owl:someValuesFrom :SovereignOrganization
                         ] ;
         owl:disjointWith :ProcessGroup ;
         rdfs:comment """A Process is a ParticipationSpace that allows participants to interact with one another in different phases in a collective decision making effort.

A Process is promoted by a SovereignOrganization and can contain in different phases different kinds of functionalities, such as Debates, MeetingCalendars, ParticipatoryBudgets or Accountability."""@en ;
         rdfs:label "Process"@en .


###  http://ontology.decidim.org/decidim_roadmap/ProcessGroup
:ProcessGroup rdf:type owl:Class ;
              rdfs:subClassOf <http://ontology.decidim.org/decidim_roadmap/#ParticipationSpace> ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :has_promoter ;
                                owl:someValuesFrom :SovereignOrganization
                              ] ;
              rdfs:comment "A GroupOfProcesses groups together different processes promoted by the same SovereignOrganization."@en ;
              rdfs:label "Process group"@en .


