@prefix : <http://ontology.decidim.org/decidim_roadmap/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://ontology.decidim.org/decidim_roadmap/> .

<http://ontology.decidim.org/decidim_roadmap/> rdf:type owl:Ontology ;
                                                owl:versionIRI : ;
                                                rdfs:comment """OWL ontology for participatory democracy in Decidim.

It provides a formal definition of the types, properties, and interrelationships of the entities that constitute the socio-technical ecosystem of participative democracy in the context of the Decidim platform.

The ontology is formalized and written using the machine-processable language RDF/OWL (Web Ontology Language) for knowledge representation.

This version of the ontology describes not only the current functionalities of Decidim, but also the functionalitites under development and foreseen in the development roadmap."""@en ;
                                                rdfs:label "Decidim Ontology"@en ;
                                                owl:versionInfo "1.0.0"@en .

#################################################################
#    Object Properties
#################################################################

###  http://ontology.decidim.org/decidim_roadmap/acts_on
:acts_on rdf:type owl:ObjectProperty ;
         owl:inverseOf :has_actor ;
         rdfs:domain <http://ontology.decidim.org/decidim_roadmap/#Agent> ;
         rdfs:comment """General property that defines the relationship between an Agent and an entity on which it perform some action.

Inverse property of has_actor and it has domain Agent."""@en ;
         rdfs:label "acts on"@en .


###  http://ontology.decidim.org/decidim_roadmap/administrator_of
:administrator_of rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :acts_on ;
                  owl:inverseOf :has_administrator ;
                  rdfs:domain <http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant> ;
                  rdfs:range :ParticipationArtifact ;
                  rdfs:label "administrator of"@en .


###  http://ontology.decidim.org/decidim_roadmap/amendment_of
:amendment_of rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :refers_to ;
              owl:inverseOf :has_amendment ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain :Amendment ;
              rdfs:range :Proposal ;
              rdfs:label "amendment of"@en .


###  http://ontology.decidim.org/decidim_roadmap/attendee_of
:attendee_of rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :acts_on ;
             owl:inverseOf :has_attendee ;
             rdfs:domain :RegisteredPerson ;
             rdfs:range :Meeting ;
             rdfs:label "attendee of"^^xsd:string .


###  http://ontology.decidim.org/decidim_roadmap/belong_to
:belong_to rdf:type owl:ObjectProperty ;
           owl:inverseOf :includes ;
           rdfs:comment """This general property links an entity to another entity it belongs to.

It is inverse property of property includes."""@en ;
           rdfs:label "belongs to"@en .


###  http://ontology.decidim.org/decidim_roadmap/category_of
:category_of rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :specifies ;
             owl:inverseOf :has_category ;
             rdfs:domain :Category ;
             rdfs:range :ParticipationItem ;
             rdfs:label "category of"@en .


###  http://ontology.decidim.org/decidim_roadmap/collaborator_of
:collaborator_of rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :acts_on ;
                 owl:inverseOf :has_collaborator ;
                 rdfs:domain <http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant> ;
                 rdfs:range <http://ontology.decidim.org/decidim_roadmap/#RegisteredParticipant> ;
                 rdfs:label "collaborator of"@en .


###  http://ontology.decidim.org/decidim_roadmap/comment_voted_by
:comment_voted_by rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :has_actor ;
                  owl:inverseOf :votes_comment ;
